{
    "manifest_version": 3,
    "name": "Space View",
    "version": "1.0",
    "permissions": ["storage", "scripting", "activeTab", "unlimitedStorage"],
   
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "128": "icon.png"
    },
    "action": {
      "default_icon": {
        "128": "icon.png"
      }
    }
  }
  